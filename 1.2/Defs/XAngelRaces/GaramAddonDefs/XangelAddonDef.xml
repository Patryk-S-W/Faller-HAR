<?xml version="1.0" encoding="utf-8" ?>
<Defs>
<AlienRace.AddonDef>
	<defName>XangelLight</defName>
    <shaderType>Cutout</shaderType>
		<!--Path-->
    <addonPath>
		<normal>Things/XAngelRaces/Lights/Light</normal>
		<damaged>Things/XAngelRaces/Lights/Light</damaged>
		<customs>
			<li>
				<hediffDef>MissingBodyPart</hediffDef>
				<path>Things/XAngelRaces/Null</path>
			</li>
		</customs>
	</addonPath>
    <!--Draw-->
	<linkedBodyPart>XAngelLightOutSide</linkedBodyPart>
<!--欠損で消える対応Parts
    頭部欠損で一緒に消えて欲しいだけなら
    <drawingToBody>をfalseにしてNoneを指定-->
    <drawnInBed>true</drawnInBed>
    <rotting>false</rotting>
    <!--Position-->
    <drawingToBody>false</drawingToBody>
	<drawPriority>	<!--パーツの表示優先度 レイヤーマップを見ると良い-->
		<north>125</north>
		<south>5</south>
		<west>125</west>
		<east>125</east>
	</drawPriority>
	<offsets>
		<north>(0.0, 0.0)</north>
		<south>(0.0, 0.0)</south>
		<west>(0, 0.0)</west>
		<east>(0, 0.0)</east>
	</offsets>
</AlienRace.AddonDef>


