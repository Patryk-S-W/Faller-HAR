		<normal>Things/XAngelRaces/Heads/Maru</normal>
		<damaged>Things/XAngelRaces/Heads/Maru</damaged>
		<customs>
<!--
			<li>
				<hediffDef>Pregnant</hediffDef>
				<path>Path</path>
				<priority>0</priority>
			</li>
-->
		</customs>
	</headPath>
	<skullPath>Things/XAngelRaces/Null</skullPath>
	<stumpPath>Things/XAngelRaces/Null</stumpPath>
	<crownType>Average</crownType>	<!--[Average]:元のテクスチャ出力（推奨） / [Narrow]:狭いテクスチャ出力-->
<!--Position-->
	<headTargetingOffsets>
		<body_south>
			<south>(0.0, 0)</south>
			<west>(-0.02, 0)</west>
			<east>(0.02, 0)</east>
		</body_south>
		<body_north>
			<north>(0.0, 0)</north>
			<west>(-0.02, 0)</west>
			<east>(0.02, 0)</east>
		</body_north>
		<body_west>
			<north>(0.02, 0)</north>
			<south>(0.02, 0)</south>
			<west>(0, 0)</west>
		</body_west>
		<body_east>
			<north>(-0.02, 0)</north>
			<south>(-0.02, 0)</south>
			<east>(0, 0)</east>
		</body_east>
	</headTargetingOffsets>
</Garam_RaceAddon.TorsoDef>



