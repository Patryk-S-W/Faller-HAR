<Garam_RaceAddon.TorsoDef Abstract="True" Name="XAngelTorso_Base">
	<shaderType>Cutout</shaderType>
	<linkedBodyGroup>Torso</linkedBodyGroup>		<!--グループに属するBodyPartにダメージが与えられた場合、damageedPathが適用されます.-->
